{
  "extends": ["tslint-react"],
  "rules": {
    "semicolon": [false, "never"],
    "indent": [ true, "spaces" ],
    "triple-equals": [ true, "allow-null-check" ],
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "jsx-alignment": [false],
    "quotemark": [true, "single", "jsx-double"]
  }
}